{
  "name": "kubejs-item-completion",
  "displayName": "kubejs-item-completion",
  "publisher": "leodreamer",
  "description": "Extension for block/item/fluid id completion in KubeJS",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "repository": {
    "type": "git",
    "url": ""
    },
  "categories": [
    "Language Packs"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ]
      }
    ],
    "commands": [
      {
        "command": "kubejs-helper.editVocabulary",
        "title": "Edit Minecraft Vocabulary"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}